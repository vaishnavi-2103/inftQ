//Implement a program to calculate the factorial of a given number.

 //import java.util.Scanner;
class Tester {
	public static void main(String[] args) {
	    //Scanner in=new Scanner(System.in);
	    int num=5;
	    int a=1,i;
	    for (i=1;i<=num ;i++ ) {
	     a*=i;
	  
	    }
	    System.out.println(a);
	}
}

